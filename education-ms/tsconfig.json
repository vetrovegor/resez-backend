{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "target": "ES2021",
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": "./",
        "incremental": true,
        "skipLibCheck": true,
        "strictNullChecks": false,
        "noImplicitAny": false,
        "strictBindCallApply": false,
        "forceConsistentCasingInFileNames": false,
        "noFallthroughCasesInSwitch": false,
        "paths": {
            "@auth": ["src/auth/"],
            "@auth/*": ["src/auth/*"],
            "@subject": ["src/subject/"],
            "@subject/*": ["src/subject/*"],
            "@score-conversion": ["src/score-conversion/"],
            "@score-conversion/*": ["src/score-conversion/*"],
            "@subject-task": ["src/subject-task/"],
            "@subject-task/*": ["src/subject-task/*"],
            "@sub-theme": ["src/sub-theme/"],
            "@sub-theme/*": ["src/sub-theme/*"],
            "@task": ["src/task/"],
            "@task/*": ["src/task/*"],
            "@upload": ["src/upload/"],
            "@upload/*": ["src/upload/*"],
            "@rabbit-mq": ["src/rabbit-mq/"],
            "@rabbit-mq/*": ["src/rabbit-mq/*"],
            "@test": ["src/test/"],
            "@test/*": ["src/test/*"],
            "@utils": ["src/utils/"],
            "@utils/*": ["src/utils/*"],
            "@test-history": ["src/test-history/"],
            "@test-history/*": ["src/test-history/*"],
            "@task-attempt": ["src/task-attempt/"],
            "@task-attempt/*": ["src/task-attempt/*"],
            "@log": ["src/log/"],
            "@log/*": ["src/log/*"],
            "@user": ["src/user/"],
            "@user/*": ["src/user/*"],
            "@task-analysis": ["src/task-analysis/"],
            "@task-analysis/*": ["src/task-analysis/*"],
            "@snippet": ["src/snippet/"],
            "@snippet/*": ["src/snippet/*"],
            "@comment/*": ["src/comment/*"],
            "@shared/*": ["src/shared/*"]
        }
    }
}
